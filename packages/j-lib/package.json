    "type-check": "vue-tsc --build"
  },
  "devDependencies": {
    "vite-plugin-dts": "^4.5.3"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.5",
    "defu": "^6.1.4",
    "pathe": "^2.0.3",
    "tailwindcss": "^4.1.5",
    "tinyglobby": "^0.2.13",
    "unplugin": "^2.3.2",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-vue-components": "^28.5.0"
  }
